<%- include('../layout/adminheader') %>

<h2 class="page-title">Add Category</h2>
<a href="/admin/categories" class="btn btn-primary">Back to all Categories</a>
<br><br>

<form method="post" action="/admin/categories/add-category"> 
    <div class="form-group">
        <label for="">Title</label>
        <input type="text" class="form-control" name="title"  placeholder="Title" required>
    </div>
    <div class="form-group">
        <label for="">Slug</label>
        <input type="text" class="form-control" name="slug"  placeholder="Slug" required>
    </div>
    <button type="submit" class="btn btn-danger">Sumbit</button>
</form>




<%- include('../layout/adminfooter') %>
